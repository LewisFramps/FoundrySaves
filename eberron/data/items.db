{"img":"systems/pf2e/icons/equipment/weapons/dwarven-war-axe.webp","name":"Dwarven War Axe","system":{"description":{"gm":"","value":"<p>This favored weapon of the dwarves has a large, ornate head mounted on a thick handle. This powerful axe can be wielded with one hand or two.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"rules":[],"slug":"dwarven-war-axe","schema":{"version":0.835,"lastMigration":{"datetime":"2023-05-26T19:58:02.021+01:00","version":{"schema":0.815,"foundry":"10.291","system":"4.10.4"}}},"level":{"value":0},"traits":{"value":["dwarf","sweep","two-hand-d12"],"rarity":"uncommon","otherTags":[]},"quantity":1,"baseItem":"dwarven-war-axe","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"2"},"equippedBulk":{"value":""},"price":{"value":{"gp":3}},"equipped":{"carryType":"worn","invested":null,"handsHeld":0},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":"silver"},"preciousMaterialGrade":{"value":"low"},"size":"med","identification":{"status":"identified","unidentified":{"name":"Unusual Dwarven War Axe","img":"systems/pf2e/icons/unidentified_item_icons/weapon.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"category":"advanced","group":"axe","bonus":{"value":0},"damage":{"dice":1,"die":"d8","damageType":"slashing","persistent":null},"bonusDamage":{"value":0},"splashDamage":{"value":0},"range":null,"reload":{"value":""},"MAP":{"value":""},"potencyRune":{"value":1},"strikingRune":{"value":null},"specific":{"value":false},"propertyRune1":{"value":null},"propertyRune2":{"value":null},"propertyRune3":{"value":null},"propertyRune4":{"value":null},"property1":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","strikeConditionType":"","strikeConditionValue":null,"criticalConditionType":"","criticalConditionValue":null}},"type":"weapon","flags":{"core":{"sourceId":"Compendium.pf