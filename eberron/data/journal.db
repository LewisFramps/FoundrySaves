{"name":"Silvertide","sort":0,"flags":{"core":{"sheetClass":"foundryvtt-simple-calendar.NoteSheet"},"foundryvtt-simple-calendar":{"noteData":{"calendarId":"default","startDate":{"year":998,"month":6,"day":13,"hour":0,"minute":0,"seconds":0},"endDate":{"year":998,"month":6,"day":13,"hour":0,"minute":0,"seconds":0},"allDay":true,"repeats":3,"order":0,"categories":["Silver Flame Holiday"],"remindUsers":[],"fromPredefined":true}}},"folder":"NE8EGbiGPykuuOlP","ownership":{"3WjUNOtdq6iPt0l2":3,"default":2},"pages":[{"name":"Silvertide","type":"text","title":{"show":false,"level":1},"text":{"format":1,"content":"<p>This highest holy day of the faith celebrates the sacrifice of the couatls and the birth of the Silver Flame.</p>"},"_id":"KW9f0ZMPhaQ00FGj","image":{},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"sort":0,"ownership":{"default":-1},"flags":{}}],"_stats":{"systemId":"pf2e","systemVersion":"4.6.8","coreVersion":"10.291","createdTime":1674945339305,"modifiedTime":1674945339305,"lastModifiedBy":"3WjUNOtdq6iPt0l2"},"_id":"pDG24f4xYXg4YYgL"}
